{
  "name": "ui-good",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prepush": "node tools/preflight.mjs && npx tsc --noEmit && next build",
    "typecheck": "tsc --noEmit",
    "lint": "node -e \"console.log('lint temporarily disabled due to rushstack patch crash')\"",
    "bootstrap:knowledge": "node scripts/bootstrap-knowledge.mjs",
    "kb": "node scripts/knowledge-cli.mjs",
    "build": "next build",
    "start": "next start",
    "prepare": "node -e \"const fs=require('fs'); const isCI=process.env.CI||process.env.VERCEL; if(isCI) process.exit(0); try{fs.statSync('.git');}catch(e){process.exit(0);}\" && npx simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-push": "npm run prepush"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.34.1",
    "chevrotain": "^11.0.3",
    "next": "15.5.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.9",
    "postcss": "^8.5.6",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "eslint": "8.57.0",
    "@rushstack/eslint-patch": "1.10.4"
  },
  "engines": {
    "node": ">=18.18 <21"
  },
  "packageManager": "npm@10.9.0"
}
