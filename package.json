{
  "name": "ui-good",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepush": "node tools/preflight.mjs && tsc --noEmit && eslint . --max-warnings=0 && next build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "build": "next build",
    "prepare": "npx simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-push": "npm run prepush"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3"
  }
}
